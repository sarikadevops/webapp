
pipeline {
agent{
  node{
      label"built-in"
      }
     }
     stages{
           stage('git checkout'){
         steps{
               git 'https://github.com/sarikadevops/webapp.git'
               }
               }
            stage('mvn'){
                  steps{
                        sh 'mvn clean install'
                       }
                          }
            
