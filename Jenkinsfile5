pipeline {
agent{
  node{
      label"built-in"
      }
     }
     triggers {   
              cron('H/2 * * * *')
              }
     stages{
            stage('mvn'){
                  steps{
                        sh 'mvn clean install'
                       }
                          }
       stage('git checkout'){
         steps{
               git 'https://github.com/sarikadevops/webapp.git'
               }
               }
     }    
      
     


